{
    "name": "easi-power/laravel-db2",
    "description": "laravel-db2 is a simple DB2 service provider for Laravel. It provides DB2 Connection by extending the Illuminate Database component of the laravel framework.",
    "keywords": [
        "laravel",
        "DB2",
        "Database",
        "PDO",
        "ODBC"
    ],
    "license": "MIT",
    "authors": [
    {
        "name": "Maxime Rault",
        "role": "Developer"
    },
    {
        "name": "Mario Aquino",
        "role": "Developer"
    },
    {
        "name": "Levi Vanheel",
        "role": "Developer"
    }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^10.0 || ^11.0",
        "ext-pdo": "*",
        "ext-iconv": "*"
    },
    "autoload": {
    "psr-4": {
        "Easi\\DB2\\": "src/"
    }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Easi\\DB2\\DB2ServiceProvider"
            ]
        }
    }
}
